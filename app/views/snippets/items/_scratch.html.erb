  <div class="main-area">
    <div class="body" style="height: 415px;">
      <iframe allowtransparency="true" width="485" height="402" src="http://scratch.mit.edu/projects/embed/<%= content_id %>?autostart=false" frameborder="0" allowfullscreen></iframe>
    </div>
    <%= render(partial: 'snippets/items/footer_web', locals: {snippet: snippet}) %>
  </div>
</div>
